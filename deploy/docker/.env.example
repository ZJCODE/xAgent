# This is an example of environment variables needed for the application.
# Copy this file to .env and fill in the required values.

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Redis URL for persistence
REDIS_URL=your_redis_url_with_password

# LangFuse observability keys
LANGFUSE_SECRET_KEY=your_langfuse_key
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_HOST=https://cloud.langfuse.com

# AWS S3 credentials for image uploads
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
BUCKET_NAME=your_bucket_name

# Proxy settings（if needed）
http_proxy=http://192.168.1.x:7890
https_proxy=http://192.168.1.x:7890